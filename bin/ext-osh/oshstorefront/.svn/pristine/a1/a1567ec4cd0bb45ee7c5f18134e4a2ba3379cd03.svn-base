.ls_distancekmi { clear:both; padding:2px; color:#777}
.ls_store_name{font-weight:bold; color:#000; display:block; font-size:16px; font-family:'liberation_serifregular'; margin:0 0 5px}
.ls_store_name a{ font-weight:bold; color:#1155CC;font-size:16px; font-family:'liberation_serifregular'; }
.ls_store_name a:hover{ text-decoration:underline; color:red}
.ls_getdirection { margin:2px 0}
.gmap_leftcolume a{ color:#1155CC; text-decoration:none}
.gmap_leftcolume a:hover{ text-decoration:underline}
.ls_tel_number { clear:both; margin:0px 0 0px}
.gmap_mainwrapper td{ vertical-align:top; }
.maker_kmi {width:70px; text-align:center}
.gmap_leftcolume td{ padding:10px 2px}
.ls_workinghours { line-height:20px; margin:0px 0 0; color:#666}
.store_hours { width:120px}
.ls_make_mystore { margin:0 0 10px; display:block; clear:both}
.gmapresult_wrapper tr.sl_list:hover td{ background:#EBF2FC} 
.gmapresult_wrapper { overflow:auto; height:605px;padding:0 0px 0 0}

/* Google Map Direction */
.gma_sideheading { background:#F2F2F2; border:#999 solid 1px; padding:5px; color:#000; font-weight:bold; font-size:12px}
.dir_search { clear:both; margin:0 0 10px; overflow:hidden}

.dir_search input{ height: 16px; line-height: 16px; padding: 3px 5px; border-radius: 1px 1px 1px 1px;border-color: #C0C0C0 #D9D9D9 #D9D9D9;border-right: 1px solid #D9D9D9;border-style: solid;border-width: 1px;width:250px}
.dir_search input:focus { border: 1px solid #4D90FE; box-shadow: 0 1px 2px rgba(0, 0, 0, 0.3) inset; outline: medium none;}
#get_direction{ }
.icon_searchadd1 { background:url(../images/store_locator/dl6.png) no-repeat 0 -140px; width:24px; height:24px; float:left; margin:0 5px 0 0;}
.icon_searchadd2 { background:url(../images/store_locator/dl6.png) no-repeat 0 -71px; width:24px; height:24px; float:left; margin:0 5px 0 0;}
.btn_direction {
    border: 3px solid #004F2F;
    float: left; margin:0 0 0 30px;
    width: auto;
}.btn_direction button {
    background-color: #004F2F;
    border: 1px solid #7AA230;
    color: #FFFFFF;
    font-family: 'neutonregular';
    font-size: 16px;
    margin: 0;
    padding: 1px;
}
.btn2 {border: 3px solid #684212;}
.btn2 a,.btn2 input{background-color: #684212; border: 1px solid #D39E5D; color: #FFFFFF; display: block; font-family: 'neutonregular';
    font-size: 16px; margin: 0; padding: 1px 4px; text-decoration: none; }
	.marbottom { margin-bottom:10px;}