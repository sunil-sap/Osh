		  p:mapping="catalog" 
		  p:converter-ref="oshBaseProductConverterForOnlineVersion"/>	 -->  
		  			  	  	
	<bean id="oshStagedVariantProductConverterMapping" 
		  class="de.hybris.platform.acceleratorservices.dataimport.batch.converter.mapping.impl.DefaultConverterMapping" 
		  p:mapping="catalog" 
		  p:converter-ref="oshVariantProductConverterForStagedVersion"/>
		  
