	 <bean id="oshVariantProductConverterForStagedVersion" class="com.hybris.osh.services.dataimport.batch.converter.impl.OshImpexConverter">
		<property name="fieldSeperator" value="|"/>
		<property name="linesToSkip" value="0"/>
		<property name="header">
			<!--  make sure to fix the header if there is a change in the header in products.impex-->
			<value>#{oshImpexProductHeader}
				$availablityIn=availabilityInd(code)[default='S']
				$productCatalog=oshProductCatalog
			    $catalogVersion=catalogversion(catalog(id[default=oshProductCatalog]),version[default='Staged'])[unique=true,default='$productCatalog:Staged']
				$supercategories=supercategories(code, $catalogVersion)
				$baseProduct=baseProduct(code, catalogVersion(catalog(id[default='$productCatalog']),version[default='Staged']))
				INSERT_UPDATE OshVariantProduct;code[unique=true];name[lang=en];$baseProduct;unit(code);approvalStatus(code);$catalogVersion;weight;availabilityInd(code);mapPriceFlag
			</value>
		</property>
		<property name="impexRow">
			<value>;{+0};{1};B-{0};pieces;{p57};;{r17};{q128};{s112}</value>
		</property>
		<property name="userService" ref="userService"/>
		<property name="customColumnTranslators">
			<map>
				<entry key="p" value-ref="approvedStatusTranslator"/>
				<entry key="q" value-ref="availabilityIndTranslator"/>
				<entry key="r" value-ref="oshDotToCommaTranslator"/>
				<entry key="s" value-ref="mapPriceTranslator"/>			
			</map>
		</property>
	</bean>
	
