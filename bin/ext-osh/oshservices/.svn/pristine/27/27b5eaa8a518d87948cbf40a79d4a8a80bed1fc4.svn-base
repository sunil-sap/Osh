	<bean id="approvedStatusTranslator" class="com.hybris.osh.services.dataimport.batch.translator.ApprovedStatusTranslator"/>
	<bean id="availabilityIndTranslator" class="com.hybris.osh.services.dataimport.batch.translator.AvailabilityIndTranslator"/>
	<bean id="oshDotToCommaTranslator" class="com.hybris.osh.services.dataimport.batch.translator.OshDotToCommaTranslator"/>
	<bean id="mapPriceTranslator" class="com.hybris.osh.services.dataimport.batch.translator.MapPriceTranslator"/>
	
