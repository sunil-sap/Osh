<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:util="http://www.springframework.org/schema/util" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
       http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
       http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-3.1.xsd"
	default-autowire="byName">
	
	
	<!-- order actions widget -->
	<alias alias="csOrderActionsWidgetConfig" name="oshdefaultCsOrderActionsWidgetConfig"/>
	<bean id="oshdefaultCsOrderActionsWidgetConfig" parent="defaultCsOrderActionsWidgetConfig">
		<property name="widgetTitle" value="cscockpit.widget.order.ordermanagement"/>
		<property name="widgetRenderer" ref="oshOrderActionsWidgetRenderer"/>
		<property name="widgetController" ref="csOrderManagementActionsWidgetController"/>
		<property name="widgetModel">
			<bean class="de.hybris.platform.cockpit.widgets.models.impl.DefaultItemWidgetModel"/>
		</property>
		<property name="widgetAdapter">
			<bean class="de.hybris.platform.cscockpit.widgets.adapters.UpdateWidgetAdapter"/>
		</property>
	</bean>

	<alias alias="oshOrderActionsWidgetRenderer" name="oshdefaultCsOrderActionsWidgetRenderer"/>
	<bean id="oshdefaultCsOrderActionsWidgetRenderer" scope="prototype" class="com.hybris.osh.oshcscockpit.widgets.renderer.impl.OshOrderManagementActionsWidgetRenderer">
		<property name="popupWidgetHelper" ref="popupWidgetHelper"/>
	</bean>
	
	<!-- voucher widget -->
	<alias alias="csVoucherWidgetConfig" name="defaultCSvoucherWidgetConfig"/>
	<bean id="defaultCSvoucherWidgetConfig" parent="csBaseWidgetConfig">
		<property name="widgetTitle" value="cscockpit.widget.checkout.voucher"/>
		<property name="widgetRenderer" ref="csVoucherWidgetConfigRenderer"/>
		<property name="widgetController" ref="csVoucherController"/>
		<property name="widgetModel">
			<bean class="de.hybris.platform.cockpit.widgets.models.impl.DefaultItemWidgetModel"/>
		</property>
		<property name="widgetAdapter">
			<bean class="de.hybris.platform.cscockpit.widgets.adapters.UpdateWidgetAdapter"/>
		</property>
	</bean>

	<alias alias="csVoucherWidgetConfigRenderer" name="defaultCsVoucherWidgetRenderer"/>
	<bean id="defaultCsVoucherWidgetRenderer" scope="prototype" class="com.hybris.osh.oshcscockpit.widgets.renderer.impl.DefaultCsVoucherRenderer">
		<property name="voucherService" ref="voucherService"/>
		<property name="basketController" ref="csBasketController"/>
		<property name="userService" ref="userService"/>
		<property name="voucherModelService" ref="voucherModelService"/>
		
	</bean>
	
	<alias alias="csVoucherController" name="defaultCsVoucherController"/>
	<bean id="defaultCsVoucherController" parent="csBaseWidgetController" class="com.hybris.osh.oshcscockpit.widget.controller.impl.DefaultCsVoucherController" scope="session">
		<property name="basketController" ref="csBasketController"/>
		<property name="voucherService" ref="voucherService"/>
	</bean>
	
	
	
	<!-- order update widget -->
	<alias alias="csOrderUpdateRequestWidgetConfig" name="defaultCsOrderUpdateWidgetConfig"/>
	<bean id="defaultCsOrderUpdateWidgetConfig" parent="csBaseWidgetConfig">
		<property name="widgetTitle" value="oshcscockpit.widget.order.updateOrder.create"/>
		<property name="widgetRenderer" ref="csOrdeUpdateWidgetRenderer"/>
		<property name="widgetController" ref="csOrderUpdateController"/>
		<property name="widgetClass" value="de.hybris.platform.cockpit.widgets.impl.DefaultInputWidget"/>
		<property name="widgetModel">
			<bean class="de.hybris.platform.cscockpit.widgets.models.impl.DefaultMasterDetailListWidgetModel"/>
		</property>
	</bean>
	
	<alias alias="csOrdeUpdateWidgetRenderer" name="defaultCsOrdeUpdateWidgetRenderer"/>
	<bean id="defaultCsOrdeUpdateWidgetRenderer" scope="prototype" class="com.hybris.osh.oshcscockpit.widgets.renderer.impl.OrderUpdateWidgetRenderer">
		<property name="productInfoConfigurationCode" value="returnsOrderEntryList"/>
		<property name="listConfigurationCode" value="updateEntryList"/>
		<property name="listEditorConfigurationCode" value="updateEntryEdit"/>
		<property name="listConfigurationType" value="Consignment"/>
		<property name="popupWidgetHelper" ref="popupWidgetHelper"/>
	</bean>
	
	<!-- Customer Credit widget renderer *******************************************  -->
	<alias alias="csCustomerCreditRequestWidgetConfig" name="defaultCsCustomerCreditWidgetConfig"/>
	<bean id="defaultCsCustomerCreditWidgetConfig" parent="csBaseWidgetConfig">
		<property name="widgetTitle" value="oshcscockpit.widget.CreditPaymentTransaction.customerCredit.create"/>
		<property name="widgetRenderer" ref="csCustomerCreditWidgetRenderer"/>
		<property name="widgetController" ref="csCustomerCreditController"/> 
		<property name="widgetClass" value="de.hybris.platform.cockpit.widgets.impl.DefaultInputWidget"/>
		<property name="widgetModel">
			<bean class="de.hybris.platform.cscockpit.widgets.models.impl.DefaultMasterDetailListWidgetModel"/>
		</property>
	</bean>
	<alias alias="csCustomerCreditWidgetRenderer" name="defaultCsCustomerCreditWidgetRenderer"/>
	<bean id="defaultCsCustomerCreditWidgetRenderer" scope="prototype" class="com.hybris.osh.oshcscockpit.widgets.renderer.impl.CustomerCreditWidgetRenderer">
	    <property name="listConfigurationCode" value="creditEntryList"/>
		<property name="listEditorConfigurationCode" value="creditEntryEdit"/> 
		<property name="listConfigurationType" value="CreditPaymentTransaction"/>
		<property name="popupWidgetHelper" ref="popupWidgetHelper"/>  
	</bean> 

   <!-- Return request widget   -->
	
	<alias alias="csRefundConfirmationWidgetRenderer" name="defaultCsRefundConfirmationWidgetRenderer"/>
	<bean id="defaultCsRefundConfirmationWidgetRenderer" scope="prototype"  class="com.hybris.osh.oshcscockpit.widgets.renderer.impl.OshRefundConfirmationWidgetRenderer">
		<property name="listConfigurationCode" value="returnsOrderEntryList"/>
		<property name="listConfigurationType" value="AbstractOrderEntry"/>
		<property name="commonI18NService" ref="commonI18NService"/>
		<property name="sessionService" ref="sessionService"/>
		<property name="popupWidgetHelper" ref="popupWidgetHelper"/>
	</bean>
	
	<alias alias="csOrderManagementActionsWidgetController" name="defaultOshOrderManagementActionsWidgetController"/>
	<bean id="defaultOshOrderManagementActionsWidgetController" parent="defaultCsOrderManagementActionsWidgetController" class="com.hybris.osh.oshcscockpit.widget.controller.impl.DefaultOshOrderManagementActionsWidgetController" scope="session">
		
	</bean>
	
	<alias alias="csOrderUpdateController" name="defaultCsOrderUpdateController"/>
	<bean id="defaultCsOrderUpdateController" parent="csBaseWidgetController" class="com.hybris.osh.oshcscockpit.widget.controller.impl.DefaultOrderUpdateController" scope="session">
		<property name="orderManagementActionsWidgetController" ref="csOrderManagementActionsWidgetController"/>
		<!-- <property name="orderController" ref="csOrderController"/> -->
		
	</bean>
	
	<alias alias="csCustomerCreditController" name="defaultCsCustomerCreditController"/>
	<bean id="defaultCsCustomerCreditController" parent="csBaseWidgetController" class="com.hybris.osh.oshcscockpit.widget.controller.impl.DefaultCustomerCreditController" scope="session">
		<property name="orderManagementActionsWidgetController" ref="csOrderManagementActionsWidgetController"/>
		<!-- <property name="orderController" ref="csOrderController"/> -->
	</bean>

	<alias alias="csOrderReturnRequestsWidgetRenderer" name="defaultOshCsOrderReturnRequestsWidgetRenderer"/>
	<bean id="defaultOshCsOrderReturnRequestsWidgetRenderer" scope="prototype" class="de.hybris.platform.cscockpit.widgets.renderers.impl.ConfigurableCsMasterDetailListboxWidgetRenderer">
		<property name="listConfigurationCode" value="returnRequestList"/>
		<property name="listConfigurationType" value="ReturnRequest"/>
		<property name="detailRenderer">
			<bean class="com.hybris.osh.oshcscockpit.widgets.renderers.details.impl.OshOrderReturnRequestListWidgetDetailRenderer">
				<property name="listConfigurationCode" value="returnRequestListDetail"/>
				<property name="listConfigurationType" value="ReturnEntry"/>
			</bean>
		</property>
	</bean> 
	
	<alias alias="csOrderConsignmentsWidgetRenderer" name="defaultOshCsOrderConsignmentsWidgetRenderer"/>
	<bean id="defaultOshCsOrderConsignmentsWidgetRenderer" scope="prototype" class="de.hybris.platform.cscockpit.widgets.renderers.impl.ConfigurableCsMasterDetailListboxWidgetRenderer">
		<property name="listConfigurationCode" value="consignmentList"/>
		<property name="listConfigurationType" value="Consignment"/>
		<property name="detailRenderer">
			<bean class="com.hybris.osh.oshcscockpit.widgets.renderer.impl.OshOrderConsignmentsWidgetDetailRenderer">
				<property name="listConfigurationCode" value="consignmentList"/>
				<property name="listConfigurationType" value="ConsignmentEntry"/>
			</bean>
		</property>
	</bean>
	
	<alias alias="csPartialOrderCancellationWidgetRenderer" name="defaultOshCsPartialOrderCancellationWidgetRenderer"/>
	<bean id="defaultOshCsPartialOrderCancellationWidgetRenderer" scope="prototype" class="com.hybris.osh.oshcscockpit.widgets.renderer.impl.OshPartialOrderCancellationWidgetRenderer">
		<property name="popupWidgetHelper" ref="popupWidgetHelper"/>
		<property name="enumerationService" ref="enumerationService"/>
		<property name="productInfoConfigurationCode" value="cancellableOrderEntryProductList"/>
	</bean>
	
	 <alias alias="csCancellationController" name="defaultOshCsCancellationController"/>
	<bean id="defaultOshCsCancellationController" parent="defaultCsCancellationController" class="com.hybris.osh.oshcscockpit.widget.controller.impl.DefaultOshCancellationController" scope="session">
	<property name="orderManagementActionsWidgetController" ref="csOrderManagementActionsWidgetController"/>
	</bean> 
	
	<alias alias="csReturnsController" name="defaultOshCsReturnsController"/>
	<bean id="defaultOshCsReturnsController" parent="defaultCsReturnsController" class="com.hybris.osh.oshcscockpit.widgets.controllers.impl.DefaultOshReturnsController" scope="session">
	</bean>
	
	
	<!-- order totals widget -->
	<alias alias="csOrderDetailOrderTotalsWidgetConfig" name="defaultOshCsOrderDetailOrderTotalsWidgetConfig"/>
	<bean id="defaultOshCsOrderDetailOrderTotalsWidgetConfig" parent="csBaseWidgetConfig">
		<property name="widgetTitle" value="cscockpit.widget.order.totals"/>
		<property name="widgetRenderer" ref="csOshOrderDetailOrderTotalsWidgetRenderer"/>
		<property name="widgetController" ref="csOrderController"/>
		<property name="widgetModel" ref="csOrderItemWidgetModel"/>
		<property name="widgetAdapter" ref="csOrderDetailsWidgetAdapter"/>
	</bean>

	<alias alias="csOshOrderDetailOrderTotalsWidgetRenderer" name="defaultOshCsOrderDetailOrderTotalsWidgetRenderer"/>
	<bean id="defaultOshCsOrderDetailOrderTotalsWidgetRenderer" scope="prototype" class="com.hybris.osh.oshcscockpit.widgets.renderer.impl.OshOrderDetailsOrderTotalWidgetRenderer"/>
	
	
	
</beans>
