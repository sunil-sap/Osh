<?xml version="1.0" encoding="UTF-8"?>
<process xmlns="http://www.hybris.de/xsd/processdefinition" start="updateTrackingId" name="consignmentUpdateWithTrackingIdSubprocess" processClass="de.hybris.platform.ordersplitting.model.ConsignmentProcessModel" onError="error">
	
	<action id="updateTrackingId" bean="sentFulfilmentEmail">
		<transition name="OK" to="sendEmail"/>
		<transition name="NOK" to="error"/>
	</action>

	<action id="sendEmail" bean="sendEmail">
		<transition name="OK" to="removeSentEmail"/>
		<transition name="NOK" to="failed"/>
	</action>

	<action id="removeSentEmail" bean="removeSentEmail">
		<transition name="OK" to="success"/>
		<transition name="NOK" to="error"/>
	</action>
	<end id="error" state="ERROR">All went wrong.</end>
	<end id="failed" state="FAILED">Order not placed.</end>
	<end id="success" state="SUCCEEDED">Order placed.</end>
</process>