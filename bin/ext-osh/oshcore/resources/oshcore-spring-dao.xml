<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="cartRemovalDao" class="com.hybris.osh.core.dao.impl.DefaultCartRemovalDao" scope="tenant">
		<property name="flexibleSearchService" ref="flexibleSearchService"/>
	</bean>
	
	<bean id="readyToPickupReminderDao" class="com.hybris.osh.core.dao.impl.DefaultReadyToPickupReminderDao" scope="tenant"/>
	
	<bean id="cheetahMailSubscriberDao" class="com.hybris.osh.core.dao.impl.DefaultCheetahMailSubscriberDao" scope="tenant"/>
	
	<alias alias="simpleSuggestionDao" name="defaultSimpleSuggestionDao"/>
	<bean id="defaultSimpleSuggestionDao"	class="com.hybris.osh.core.suggestion.dao.impl.DefaultSimpleSuggestionDao" parent="abstractItemDao" scope="tenant">
		<property name="flexibleSearchService" ref="flexibleSearchService"/>
	</bean>
	
	<bean id="orderEntryDao" class="com.hybris.osh.core.dao.impl.DefaultOrderEntryDao" scope="tenant">
		<property name="flexibleSearchService" ref="flexibleSearchService"/>
	</bean>
	
	<bean id="oshOrderDao" class="com.hybris.osh.core.dao.impl.DefaultOshOrderDao" scope="tenant">
		<property name="flexibleSearchService" ref="flexibleSearchService"/>
	</bean>
	
	<bean id="oshRegionDao" class="com.hybris.osh.core.dao.impl.DefaultOshRegionDao" parent="baseI18NDao" scope="tenant"/>
	
	
	<bean id="findMediaContainerDao" class="com.hybris.osh.core.dao.impl.DefaultFindMediaContainerDao" scope="tenant">
		<property name="flexibleSearchService" ref="flexibleSearchService"/>
	</bean>
	
	<bean id="oshSFTPDao" class="com.hybris.osh.core.dao.impl.DefaultOshSFTPDao" scope="tenant">
		<property name="flexibleSearchService" ref="flexibleSearchService"/>
	</bean>
	
	<alias name="defaultOshCustomerAccountDao" alias="customerAccountDao"/>
	<bean id="defaultOshCustomerAccountDao" class="com.hybris.osh.core.customer.dao.impl.DefaultOshCustomerAccountDao" parent="defaultCustomerAccountDao" scope="tenant">
	</bean> 
	
	<bean id="oshPromotionDao" class="com.hybris.osh.core.dao.impl.DefaultOshPromotionDao" scope="tenant">
		<property name="flexibleSearchService" ref="flexibleSearchService"/>
	</bean>
	<bean id="defaultOshDeliveryModeDao" class="com.hybris.osh.core.dao.impl.DefaultOshDeliveryModeDao" parent="defaultDeliveryModeDao" scope="tenant"/>
	
	<bean id= "oshStoreFlagDao" class="com.hybris.osh.core.dao.impl.DefaultOshStoreFlagDao" scope="tenant"></bean>
</beans>