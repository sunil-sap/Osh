<?xml version="1.0" encoding="UTF-8"?>

<wizard-config showPrefilledValues="false" selectMode="true" createMode="true" displaySubtypes="true">
	<displayed-properties>
		<group qualifier="General" visible="true" initially-opened="true">
			<label key="cockpit.config.label.General" />
			<property qualifier="CMSItem.name"/>
		</group>
		<group qualifier="Properties" visible="true" initially-opened="true">
			<label key="config.wizardConfig.properties" />
			<property qualifier="AbstractBannerComponent.media" editorCode="simpleMediaReferenceSelector" >
				<parameter>		
					<name>restrictToCreateTypes</name>
					<value>Media,CelumAsset+</value>
			</parameter>
			</property>
			<property qualifier="ImageMapComponent.imageMapHTML" editorCode="default">
				<parameter name="rows" value="10"/>
			</property>
			<property qualifier="AbstractBannerComponent.urlLink"/>
			<property qualifier="AbstractBannerComponent.external" />
		</group>
	</displayed-properties>
</wizard-config>
