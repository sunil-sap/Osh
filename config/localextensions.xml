<?xml version="1.0" encoding="ISO-8859-1"?>

<!--
   extensions.xml - hybris Multichannel Accelerator Standalone

   Note: If a file localextensions.xml is available, it is used instead of this file.

   this file configures which hybris Platform extensions are installed,
   each <extension dir=".."> points to an extension directory. The path can be
   absolute or relative.

   After adding an extension, call 'ant' to build the system.
-->

<hybrisconfig xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="resources/schemas/extensions.xsd">

	<extensions>

		<!-- All platform extensions will be included automatically. -->
		

		<!-- ext-commerce -->

		<extension dir="${HYBRIS_BIN_DIR}/ext-commerce/basecommerce"/>
		<extension dir="${HYBRIS_BIN_DIR}/ext-commerce/btg"/>
		<extension dir="${HYBRIS_BIN_DIR}/ext-commerce/btgcockpit"/>
		<extension dir="${HYBRIS_BIN_DIR}/ext-commerce/commercefacades" />
		<extension dir="${HYBRIS_BIN_DIR}/ext-commerce/commerceservices" />
		<extension dir="${HYBRIS_BIN_DIR}/ext-commerce/customerreview"/>
		<extension dir="${HYBRIS_BIN_DIR}/ext-commerce/cybersource"/>
		<extension dir="${HYBRIS_BIN_DIR}/ext-commerce/hygocockpit"/>
		<extension dir="${HYBRIS_BIN_DIR}/ext-commerce/hygosearch"/>
		<extension dir="${HYBRIS_BIN_DIR}/ext-commerce/payment"/>
		<extension dir="${HYBRIS_BIN_DIR}/ext-commerce/promotions"/>
		<extension dir="${HYBRIS_BIN_DIR}/ext-commerce/solrfacetsearch"/>		
		<extension dir="${HYBRIS_BIN_DIR}/ext-commerce/ticketsystem"/>		
		<extension dir="${HYBRIS_BIN_DIR}/ext-commerce/voucher"/>		
		<extension dir="${HYBRIS_BIN_DIR}/ext-commerce/wishlist"/>


		<!-- ext-content -->

		<extension dir="${HYBRIS_BIN_DIR}/ext-content/classificationsystems"/>
		<extension dir="${HYBRIS_BIN_DIR}/ext-content/cms2"/>
		<extension dir="${HYBRIS_BIN_DIR}/ext-content/cms2lib"/>
		<extension dir="${HYBRIS_BIN_DIR}/ext-content/cmscockpit"/>
		<extension dir="${HYBRIS_BIN_DIR}/ext-content/productcockpit"/>
		<extension dir="${HYBRIS_BIN_DIR}/ext-content/mediaconversion"/>
		<extension dir="${HYBRIS_BIN_DIR}/ext-content/importcockpit"/>

		<!-- ext-channel -->

		<extension dir="${HYBRIS_BIN_DIR}/ext-channel/cscockpit"/>
		<extension dir="${HYBRIS_BIN_DIR}/ext-channel/reportcockpit"/>


		<!-- ext-accelerator -->
		
		<extension dir="${HYBRIS_BIN_DIR}/ext-accelerator/acceleratorcms" />
		<extension dir="${HYBRIS_BIN_DIR}/ext-accelerator/acceleratorfacades" />
		<!-- <extension dir="${HYBRIS_BIN_DIR}/ext-accelerator/acceleratorsampledata" /> -->
		<extension dir="${HYBRIS_BIN_DIR}/ext-accelerator/acceleratorservices" />

		
		<!-- ext-template -->
		
		<!-- <extension dir="${HYBRIS_BIN_DIR}/ext-template/fulfilmentprocess"/> -->

		<!-- osh  -->
 
		<extension dir="${HYBRIS_BIN_DIR}/ext-osh/oshcockpits"/>
		<extension dir="${HYBRIS_BIN_DIR}/ext-osh/oshcore"/>
		<extension dir="${HYBRIS_BIN_DIR}/ext-osh/oshfacades"/>
		<extension dir="${HYBRIS_BIN_DIR}/ext-osh/oshinitialdata"/>
		<extension dir="${HYBRIS_BIN_DIR}/ext-osh/oshstorefront"/>
		<extension dir="${HYBRIS_BIN_DIR}/ext-osh/oshtest"/>
		<extension dir="${HYBRIS_BIN_DIR}/ext-osh/oshfulfilment"/>
 		<extension dir="${HYBRIS_BIN_DIR}/ext-osh/oshcscockpit"/>
 		<extension dir="${HYBRIS_BIN_DIR}/ext-osh/oshcybersource"/>
 		<extension dir="${HYBRIS_BIN_DIR}/ext-osh/oshservices"/>
	</extensions>

</hybrisconfig>
